{
  "expo": {
    "name": "RapNow!",
    "slug": "rapnow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/micorphone.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow rapnow! to access your microphone."
        }
      ]
    ],
    "splash": {
      "image": "./assets/rapnow.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
